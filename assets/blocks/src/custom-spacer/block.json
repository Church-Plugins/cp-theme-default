{
	"name": "lmpc/spacer",
	"title": "Spacer 2",
	"apiVersion": 2,
	"category": "widgets",
	"description": "Add spacing between blocks",
	"supports": {
		"anchor": true,
		"spacing": {
			"margin": [ "top", "bottom" ],
			"__experimentalDefaultControls": {
				"margin": true
			}
		}
	},
  "attributes": {
		"height": {
			"type": "string",
			"default": "var:preset|spacing|dynamic"
		},
		"width": {
			"type": "string"
		}
	},
	"textdomain": "lmpc"
}
