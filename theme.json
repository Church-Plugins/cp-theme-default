{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 2,
  "settings": {
    "useRootPaddingAwareAlignments": false,
    "appearanceTools": false,
    "typography": {  
      "fontSizes": [],
      "fontFamilies": [],
      "customFontSize": false,
      "lineHeight": false,
      "textDecoration": false,
      "letterSpacing": false,
      "fontStyle": false,
      "fontWeight": false,
      "textTransform": false
    },
    "color": {
      "background": true,
      "custom": false,
      "defaultPalette": false,
      "palette": [
        {
          "name": "Primary Brand",
          "slug": "primary-brand",
          "color": "#1C274F"
        },
        {
          "name": "Secondary",
          "slug": "secondary",
          "color": "#378A8A"
        },
        {
          "name": "Inverted",
          "slug": "inverted",
          "color": "#CADCEE"
        },
        {
          "name": "Text Primary",
          "slug": "text-primary",
          "color": "#14151B"
        },
        {
          "name": "Text Secondary",
          "slug": "text-secondary",
          "color": "#5F5B64"
        },
        {
          "name": "Text Tertiary",
          "slug": "text-tertiary",
          "color": "#76737A"
        },
        {
          "name": "Disabled",
          "slug": "disabled",
          "color": "#A2A1A4"
        },
        {
          "name": "Info",
          "slug": "info",
          "color": "#23303D"
        },
        {
          "name": "Success",
          "slug": "success",
          "color": "#378A8A"
        },
        {
          "name": "Warning",
          "slug": "warning",
          "color": "#D6774B"
        },
        {
          "name": "Danger",
          "slug": "danger",
          "color": "#E3633B"
        },
        {
          "name": "White",
          "slug": "white",
          "color": "#ffffff"
        },
        {
          "name": "Black",
          "slug": "black",
          "color": "#000000"
        }
      ],
      "text": true
    },
    "spacing": {
      "margin": false,
      "padding": false,
      "blockGap": false,
      "spacingSizes": [
        {
          "size": "var(--cp-gap--sm)",
          "slug": "sm",
          "name": "S"
        },
        {
          "size": "var(--cp-gap--main)",
          "slug": "dynamic",
          "name": "Dynamic"
        },
        {
          "size": "var(--cp-gap--md)",
          "slug": "md",
          "name": "M"
        },
        {
          "size": "var(--cp-gap--lg)",
          "slug": "lg",
          "name": "L"
        },
        {
          "size": "var(--cp-gap--xl)",
          "slug": "xl",
          "name": "XL"
        }
      ],
      "customSpacingSize": false
    },
    "layout": {
      "definitions": {
				"default": {
					"name": "default",
					"slug": "flow",
					"className": "is-layout-flow",
					"baseStyles": [
						{
							"selector": " > .alignleft",
							"rules": {
								"float": "left",
								"margin-inline-start": "0",
								"margin-inline-end": "2em"
							}
						},
						{
							"selector": " > .alignright",
							"rules": {
								"float": "right",
								"margin-inline-start": "2em",
								"margin-inline-end": "0"
							}
						},
						{
							"selector": " > .aligncenter",
							"rules": {
								"margin-left": "auto !important",
								"margin-right": "auto !important"
							}
						}
					],
					"spacingStyles": [
						{
							"selector": " > *",
							"rules": {
                "margin-block-start": "x",
                "margin-block-end": "x"
              }
						},
						{
							"selector": " > * + *",
							"rules": {
                "margin-block-start": "x",
                "margin-block-end": "x"
              }
						}
					]
				},
				"constrained": {
					"name": "constrained",
					"slug": "constrained",
					"className": "is-layout-constrained",
					"baseStyles": [
						{
							"selector": " > .alignleft",
							"rules": {
								"float": "left",
								"margin-inline-start": "0",
								"margin-inline-end": "2em"
							}
						},
						{
							"selector": " > .alignright",
							"rules": {
								"float": "right",
								"margin-inline-start": "2em",
								"margin-inline-end": "0"
							}
						},
						{
							"selector": " > .aligncenter",
							"rules": {
								"margin-left": "auto !important",
								"margin-right": "auto !important"
							}
						},
						{
							"selector": " > :where(:not(.alignleft):not(.alignright):not(.alignfull))",
							"rules": {
								"max-width": "var(--wp--style--global--content-size)",
								"margin-left": "auto !important",
								"margin-right": "auto !important"
							}
						},
						{
							"selector": " > .alignwide",
							"rules": {
								"max-width": "var(--wp--style--global--wide-size)"
							}
						}
					],
					"spacingStyles": [
						{
							"selector": " > *",
							"rules": {
                "margin-block-start": "x",
                "margin-block-end": "x"
              }
						},
						{
							"selector": " > * + *",
							"rules": {
                "margin-block-start": "x",
                "margin-block-end": "x"
              }
						}
					]
				},
				"flex": {
					"name": "flex",
					"slug": "flex",
					"className": "is-layout-flex",
					"displayMode": "flex",
					"baseStyles": [
						{
							"selector": "",
							"rules": {
								"flex-wrap": "wrap",
								"align-items": "center"
							}
						},
						{
							"selector": " > *",
							"rules": {}
						}
					],
					"spacingStyles": [
						{
							"selector": "",
							"rules": {
								"gap": null
							}
						}
					]
				}
			},
      "contentSize": "71rem",
			"wideSize": "75rem"
    },
    "blocks": {
      "core/heading": {
        "typography": {
          "fontSizes": []
        }
      },
      "core/button": {
        "color": {
          "text": false,
          "background": false,
          "link": false,
          "custom": false,
          "customDuotone": false,
          "customGradient": false,
          "defaultDuotone": false,
          "defaultGradients": false,
          "defaultPalette": false
        },
        "border": {
          "color": false,
          "radius": false,
          "style": false,
          "width": false
        }
      },
      "core/paragraph": {
        "typography": {
          "fontSizes": [
            {
              "name": "Small",
              "size": "var(--cp-font-size--xs)",
              "slug": "extra-small"
            },
            {
              "name": "Medium",
              "size": "var(--cp-font-size--sm)",
              "slug": "small"
            },
            {
              "name": "Large",
              "size": "var(--cp-font-size)",
              "slug": "medium"
            },
            {
              "name": "Extra Large",
              "size": "var(--cp-font-size--lg)",
              "slug": "large"
            }
          ]
        }
      },
      "core/group": {
        "spacing": {
          "margin": true,
          "padding": true,
          "blockGap": true
        }
      },
      "core/columns": {
        "spacing": {
          "margin": true,
          "padding": true,
          "blockGap": true
        }
      }
    }
  },
  "styles": {
    "spacing": {
      "blockGap": "var:preset|spacing|sm"
    },
    "blocks": {
      "core/paragraph": {
        "typography": {
          "fontFamily": "var(--cp-font-family--header)",
          "fontSize": "var(--wp--preset--font-size--large)"
        }
      }
    },
    "elements": {
      "h1": {
        "typography": {
          "fontFamily": "var(--cp-font-family--header)"
        },
        "color": {
          "text": "var(--wp--preset--color--primary-brand)"
        }
      },
      "h2": {
        "typography": {
          "fontFamily": "var(--cp-font-family--header)"
        },
        "color": {
          "text": "var(--wp--preset--color--primary-brand)"
        }
      },
      "h3": {
        "typography": {
          "fontFamily": "var(--cp-font-family--header)"
        },
        "color": {
          "text": "var(--wp--preset--color--primary-brand)"
        }
      },
      "h4": {
        "typography": {
          "fontFamily": "var(--cp-font-family--base)",
          "textTransform": "uppercase"
        },
        "color": {
          "text": "var(--wp--preset--color--primary-brand)"
        }
      },
      "h5": {
        "typography": {
          "fontFamily": "var(--cp-font-family--base)",
          "textTransform": "uppercase"
        },
        "color": {
          "text": "var(--wp--preset--color--primary-brand)"
        }
      },
      "h6": {
        "typography": {
          "fontFamily": "var(--cp-font-family--base)",
          "textTransform": "uppercase"
        },
        "color": {
          "text": "var(--wp--preset--color--primary-brand)"
        }
      }
    }
  }
}