{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 2,
  "settings": {
    "typography": {
      "fontSizes": [
        {
          "name": "Small",
          "size": "var(--cp-font-size--xs)",
          "slug": "extra-small"
        },
        {
          "name": "Medium",
          "size": "var(--cp-font-size--sm)",
          "slug": "small"
        },
        {
          "name": "Large",
          "size": "var(--cp-font-size)",
          "slug": "medium"
        },
        {
          "name": "Extra Large",
          "size": "var(--cp-font-size--lg)",
          "slug": "large"
        }
      ],
      "fontFamilies": [],
      "customFontSize": false,
      "lineHeight": false
    },
    "color": {
      "background": true,
      "custom": false,
      "defaultPalette": false,
      "palette": [
        {
          "name": "Primary Brand",
          "slug": "primary-brand",
          "color": "#23303D"
        },
        {
          "name": "Secondary",
          "slug": "secondary",
          "color": "#378A8A"
        },
        {
          "name": "Inverted",
          "slug": "inverted",
          "color": "#CADCEE"
        },
        {
          "name": "Text Primary",
          "slug": "text-primary",
          "color": "#14151B"
        },
        {
          "name": "Text Secondary",
          "slug": "text-secondary",
          "color": "#5F5B64"
        },
        {
          "name": "Text Tertiary",
          "slug": "text-tertiary",
          "color": "#76737A"
        },
        {
          "name": "Disabled",
          "slug": "disabled",
          "color": "#A2A1A4"
        },
        {
          "name": "Info",
          "slug": "info",
          "color": "#23303D"
        },
        {
          "name": "Success",
          "slug": "success",
          "color": "#378A8A"
        },
        {
          "name": "Warning",
          "slug": "warning",
          "color": "#D6774B"
        },
        {
          "name": "Danger",
          "slug": "danger",
          "color": "#E3633B"
        },
        {
          "name": "White",
          "slug": "white",
          "color": "#ffffff"
        },
        {
          "name": "Black",
          "slug": "black",
          "color": "#000000"
        }
      ],
      "text": true
    },
    "spacing": {
      "margin": true,
      "padding": true,
      "blockGap": true,
      "spacingSizes": [
        {
          "size": "var(--cp-gap--sm)",
          "slug": "sm",
          "name": "S"
        },
        {
          "size": "var(--cp-gap--md)",
          "slug": "md",
          "name": "M"
        },
        {
          "size": "var(--cp-gap--lg)",
          "slug": "lg",
          "name": "L"
        }
      ]
    },
    "blocks": {
      "core/heading": {
        "typography": {
          "fontSizes": []
        }
      },
      "core/button": {
        "color": {
          "text": false,
          "background": false,
          "link": false,
          "custom": false,
          "customDuotone": false,
          "customGradient": false,
          "defaultDuotone": false,
          "defaultGradients": false,
          "defaultPalette": false
        },
        "border": {
          "color": false,
          "radius": false,
          "style": false,
          "width": false
        }
      }
    },
    "layout": {
			"contentSize": "71rem",
			"wideSize": "75rem"
		}
  },
  "styles": {
    "spacing": {
      "blockGap": "var(--cp-gap--main)"
    },
    "blocks": {
      "core/paragraph": {
        "typography": {
          "fontFamily": "var(--cp-font-family--base)",
          "fontSize": "var(--wp--preset--font-size--medium)"
        }
      }
    },
    "elements": {
      "h1": {
        "typography": {
          "fontFamily": "var(--cp-font-family--header)"
        },
        "color": {
          "text": "#000000"
        }
      },
      "h2": {
        "typography": {
          "fontFamily": "var(--cp-font-family--header)"
        },
        "color": {
          "text": "#000000"
        }
      },
      "h3": {
        "typography": {
          "fontFamily": "var(--cp-font-family--header)"
        },
        "color": {
          "text": "#000000"
        }
      },
      "h4": {
        "typography": {
          "fontFamily": "var(--cp-font-family--base)",
          "textTransform": "uppercase"
        },
        "color": {
          "text": "#000000"
        }
      },
      "h5": {
        "typography": {
          "fontFamily": "var(--cp-font-family--base)",
          "textTransform": "uppercase"
        },
        "color": {
          "text": "#000000"
        }
      },
      "h6": {
        "typography": {
          "fontFamily": "var(--cp-font-family--base)",
          "textTransform": "uppercase"
        },
        "color": {
          "text": "#000000"
        }
      }
    }
  }
}